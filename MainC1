#include <iostream>

using namespace std;

int main()
{
	int a=13, b=10, z;
	float f=4.3;
	char c='A';
	double d=2.34567;
	a=12; //giving diff. value than before
	z=a+b; //addition,z has defined int before(compulsory)
    cout<<z<<" ";
	a=a+b; //assignment
	cout<<"Hello World"<<"\n"; //adding space
    cout<<a; //assigned a will be the output ans \n for next line
	return 0;
}
